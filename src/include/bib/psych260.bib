	urldate = {2016-02-03},
	journal = {Nature Neuroscience},
	author = {Zhan, Yang and Paolicelli, Rosa C. and Sforazzini, Francesco and Weinhard, Laetitia and Bolasco, Giulia and Pagani, Francesca and Vyssotski, Alexei L. and Bifone, Angelo and Gozzi, Alessandro and Ragozzino, Davide and Gross, Cornelius T.},
	month = mar,
	year = {2014},
	keywords = {Autism spectrum disorders, Cellular neuroscience, Microglia, Neuronal physiology},
	pages = {400--406},
	file = {Snapshot:/Users/rick/Library/Application Support/Zotero/Profiles/isqcn0ks.default/zotero/storage/SMX9QF84/nn.3641.html:text/html}
}
