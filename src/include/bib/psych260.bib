	month = aug,
	year = {2014},
	keywords = {Arousal, Corticotropin releasing factor, Depression, Emotion, Hypothalamic pituitary adrenal axis, Locus coeruleus, Posttraumatic stress disorder, Sex difference, Stress},
	pages = {303--319}
}


